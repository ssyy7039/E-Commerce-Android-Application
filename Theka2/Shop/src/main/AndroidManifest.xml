<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.shivam.shop">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name="com.shivam.shop.Orderproducts"></activity>
        <activity android:name="com.shivam.shop.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.shivam.shop.Termscondtions">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.shivam.shop.Appinfo" />
        <activity android:name="com.shivam.shop.Forgotpassword" />
        <activity android:name="com.shivam.shop.Updatedetails" />
        <activity android:name="com.shivam.shop.Myaccount" />
        <activity android:name="com.shivam.shop.Chat" />
        <activity android:name="com.shivam.shop.Profile" />
        <activity android:name="com.shivam.shop.Updatedrink" />
        <activity android:name="com.shivam.shop.Drinkdetails" />
        <activity android:name="com.shivam.shop.Add" />
        <activity android:name="com.shivam.shop.Allorders" />
        <activity android:name="com.shivam.shop.Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.shivam.shop.Signin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.shivam.shop.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>